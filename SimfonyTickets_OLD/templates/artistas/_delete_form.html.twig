<form method="post" action="{{ path('app_artistas_delete', {'id': artista.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ artista.id) }}">
    <button class="btn btn-danger"><i class="material-icons">&#xE872;</i><span>Borrar</span></button>
</form>
